{
  "ConnectionStrings": {
    "UserDataSqlite": "Data Source=/home/noar/ParagliderFlightLogDb/UserDbParagliderFlightLog.db;",
    "Sqlite": "Data Source=/home/noar/ParagliderFlightLogDb/{UserId}_ParagliderFlightLog.db;"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithTreadId" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "/var/log/ParagliderFlightLog/log.txt",
          "outputTemplate": "{Timestamp:G} {Message}{NewLine:1}{Exception:1}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "urls": "http://*:5000",
  "XcScore": {
    "CalculatorCmd": "node node_modules/igc-xc-score/dist/igc-xc-score.cjs",
    "TmpFileDirectory": "/home/noar/ParagliderFlightLog/score"
  }
}
